{
  "extends": ["eslint:recommended", "prettier", "plugin:react/recommended"],
  "plugins": [],
  "parserOptions": {
    "ecmaVersion": 2022,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "react/react-in-jsx-scope": 0,
    "react/jsx-uses-vars": "error"
  }
}
